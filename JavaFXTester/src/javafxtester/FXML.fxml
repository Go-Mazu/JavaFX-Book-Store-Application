      <Text layoutX="151.0" layoutY="160.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username:" />
      <Text layoutX="153.0" layoutY="213.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password:" />
      <TextField fx:id="username" layoutX="260.0" layoutY="143.0" promptText="username" />
